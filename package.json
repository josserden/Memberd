{
  "name": "memberd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/Memberd/",
    "preview": "vite preview",
    "lint": "stylelint --fix --config .stylelintrc.json \"./src/**/*.css\"",
    "format": "prettier --write \"./src/**/*.{js,css}\"",
    "test-all": "npm run lint && npm run format",
    "prepare": "husky install"
  },
  "devDependencies": {
    "glob": "^8.1.0",
    "prettier": "2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-order-config-standard": "^0.1.3",
    "vite": "^4.0.0",
    "vite-plugin-stylelint": "^4.3.0",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "stylelint-config-standard": "^33.0.0",
    "vite-plugin-full-reload": "^1.0.5",
    "vite-plugin-html-inject": "^1.0.1"
  }
}
